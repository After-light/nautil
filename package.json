{
  "name": "nautil",
  "version": "0.33.8",
  "description": "A javascript reactive framework which is for building crossing-platform applications.",
  "main": "index.js",
  "miniprogram": "miniprogram_dist",
  "scripts": {
    "clean": "rimraf dist miniprogram_dist dom lib native ssr web-component wechat index.js",
    "build": "npm run clean && npm run build:cjs && npm run build:wechat",
    "build:cjs": "babel src --out-dir . --config-file ./.build/babel.config.js --keep-file-extension --copy-files",
    "build:wechat": "copyfiles -f src/wechat/components/dynamic/* miniprogram_dist/wechat/components/dynamic && NODE_ENV=production webpack --config .build/webpack.wechat.config.js",
    "dev": "webpack-dev-server --config .examples/app/webpack.config.js",
    "postinstall": "node .scripts/files-for-wechat.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangshuang/nautil.git"
  },
  "keywords": [
    "javascript",
    "framework",
    "react"
  ],
  "author": "tangshuang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tangshuang/nautil/issues"
  },
  "homepage": "https://github.com/tangshuang/nautil#readme",
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "@types/react": "^17.0.11",
    "i18next": "^20.3.1",
    "immer": "^9.0.3",
    "react": "^17.0.2",
    "rxjs": "^7.1.0",
    "ts-fns": "^10.5.1",
    "tyshemo": "^11.12.2"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": "^1.14.1",
    "i18next-browser-languagedetector": "^4.0.1",
    "i18next-react-native-language-detector": "^1.0.2",
    "react-dom": "^17.0.2",
    "react-native": "^0.64.0",
    "react-native-locale-detector": "^1.0.1",
    "react-reconciler": "^0.26.2",
    "scheduler": "^0.20.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.1.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^4.3.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "react-reconciler": "^0.26.2",
    "rimraf": "^3.0.2",
    "scheduler": "^0.20.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
