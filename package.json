{
  "name": "nautil",
  "version": "0.31.23-0",
  "description": "A javascript reactive framework which is for building crossing-platform applications.",
  "main": "index.js",
  "miniprogram": "dist",
  "scripts": {
    "clean": "rimraf dist dom lib native ssr web-component wechat index.js",
    "build": "npm run build:cjs && npm run build:dist && npm run build:wechat",
    "build:cjs": "npm run clean && babel src --out-dir . --config-file ./.build/babel.config.js --keep-file-extension --copy-files",
    "build:dist": "webpack --config .build/webpack.config.js",
    "build:wechat": "webpack --config .build/webpack.wechat.config.js && babel src/wechat/components --out-dir dist/wechat/components --config-file ./.build/babel.config.js --keep-file-extension --copy-files",
    "dev": "webpack-dev-server --config .examples/app/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangshuang/nautil.git"
  },
  "keywords": [
    "javascript",
    "framework",
    "react"
  ],
  "author": "tangshuang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tangshuang/nautil/issues"
  },
  "homepage": "https://github.com/tangshuang/nautil#readme",
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "i18next": "^20.2.1",
    "immer": "^9.0.1",
    "react": "^17.0.2",
    "rxjs": "^6.6.7",
    "scopex": "^2.1.1",
    "ts-fns": "^10.3.0",
    "tyshemo": "^11.2.10"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": "^1.14.1",
    "i18next-browser-languagedetector": "^4.0.1",
    "i18next-react-native-language-detector": "^1.0.2",
    "react-dom": "^17.0.2",
    "react-native": "^0.64.0",
    "react-native-locale-detector": "^1.0.1",
    "react-reconciler": "^0.26.2",
    "scheduler": "^0.20.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.3.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "react-reconciler": "^0.26.2",
    "rimraf": "^3.0.2",
    "scheduler": "^0.20.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
